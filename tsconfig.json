{
    "types": ["vite/client"],
    "compilerOptions": {
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "preserve",
        "module": "ESNext",
        "moduleResolution": "node",
        "outDir": "dist",
        "skipLibCheck": true,
        "strict": true,
        "target": "ESNext",
        "useDefineForClassFields": true
    },
    "exclude": ["node_modules"],
}
